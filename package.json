{
  "name": "tent",
  "version": "0.3.1",
  "description": "Superstar admin interface with markdown preview",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturi/tent.git"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "browser-sync": "^2.18.12",
    "bundle-collapser": "^1.2.1",
    "drag-drop": "^2.13.2",
    "express": "^4.15.3",
    "fastmatter": "^1.1.1",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.2",
    "hyperx": "^2.3.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "lodash": "^4.17.3",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^8.3.1",
    "markdown-it-task-lists": "^1.4.1",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "sharp": "^0.17.3",
    "shortid": "^2.2.8",
    "template-css": "github:arturi/template-css",
    "uglifyify": "^4.0.5",
    "preact": "^8.2.7",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "hyperxify": "^2.0.1",
    "npm-run-all": "^3.1.2",
    "watchify": "^3.8.0",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "build": "browserify -t hyperxify -t babelify -g uglifyify -p bundle-collapser/plugin index.js | uglifyjs > public/bundle.js",
    "server": "node server/index.js --port=3360",
    "watch:client": "watchify -t babelify index.js -o public/bundle.js -vd",
    "watch:server": "cd server && nodemon index.js --port=3360",
    "watch:dev-server": "browser-sync start --proxy \"localhost:3350\" --port 3351 --files \"public/**\"",
    "start:prod": "npm-run-all build server",
    "start": "npm-run-all --parallel watch:client watch:server watch:dev-server"
  },
  "keywords": [
    "admin",
    "static"
  ],
  "author": "Artur Paikin",
  "license": "MIT"
}
